<?xml version="1.0" standalone="no"?>
<project name="dgCommon" default="generateJava" basedir=".">
	<property name="wsdlUrl" value="http://localhost:8080/dgcontroller/ControllerConnectorPort?wsdl" />
	<target name="generateJava">
	  <exec executable="${java.home}/../bin/wsimport">
	  	<arg value="-extension"/>
	    <arg value="-s"/>
	  	<arg value="src"/>
	    <arg value="-d"/>
	  	<arg value="bin"/>
	    <arg value="-keep"/>
	  	<arg value="-p"/>
	  	<arg value="com.dataguise.webservices.beans"/>
	  	<arg value="${wsdlUrl}"/>
	  </exec>
	</target>
</project>